<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EstacionDB">
  <class name="EstacionDB.VO.ClienteVO" table="CLIENTES">
    <id name="IdCliente">
      <column name="ID_CLIENTE" sql-type="bigint" not-null="true"/>
      <generator class="identity"/>
    </id>
    <property name="TipoId">
      <column name="TIPO_ID"/>
    </property>
    <property name="Identificacion">
      <column name="IDENTIFICACION"/>
    </property>
    <property name="Nombre">
      <column name="NOMBRE"/>
    </property>
    <property name="Direccion">
      <column name="DIRECCION"/>
    </property>
    <property name="Telefono">
      <column name="TELEFONO"/>
    </property>
    <property name="Contacto">
      <column name="CONTACTO"/>
    </property>
    <property name="Email">
      <column name="EMAIL"/>
    </property>
    <property name="Codigo">
      <column name="CODIGO"/>
    </property>
    <property name="Consecutivo">
      <column name="CONSECUTIVO"/>
    </property>
  </class>
</hibernate-mapping>
