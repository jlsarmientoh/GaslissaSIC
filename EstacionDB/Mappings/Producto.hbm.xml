<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EstacionDB">
  <class name="EstacionDB.VO.ProductoVO" table="PRODUCTOS">
    <id name="IdProducto">
      <column name="ID_PRODUCTO" sql-type="int" not-null="true"/>
      <generator class="identity"/>
    </id>
    <property name="Nombre">
      <column name="NOMBRE"/>
    </property>
  </class>
</hibernate-mapping>
