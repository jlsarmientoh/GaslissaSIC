<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EstacionDB">
  <class name="EstacionDB.VO.ClienteVO" table="CLIENTES">
    <id name="IdCliente">
      <column name="COD_CLI" sql-type="bigint" not-null="true"/>
      <generator class="identity"/>
    </id>    
    <property name="Identificacion">
      <column name="NIT"/>
    </property>
    <property name="Nombre">
      <column name="NOMBRE"/>
    </property>
    <property name="Direccion">
      <column name="DIR_OFICINA"/>
    </property>
    <property name="Telefono">
      <column name="TEL_OFICINA"/>
    </property>    
    <property name="Consecutivo">
      <column name="IgNit"/>
    </property>
    <property name="TipoNit">
      <column name="TIPO_NIT"/>
    </property>
  </class>
</hibernate-mapping>

