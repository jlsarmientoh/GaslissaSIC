<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="EstacionDB">
  <class name="EstacionDB.VO.EmpleadoVO" table="EMPLEADO">
    <id name="Id">
      <column name="COD_EMP" sql-type="char(15)" not-null="true"/>
      <generator class="identity"/>
    </id>
    <property name="Nombre">
      <column name="NOMBRE"/>
    </property>
  </class>
</hibernate-mapping>